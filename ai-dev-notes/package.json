  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "contentlayer": "^0.3.4",
    "feed": "^5.1.0",
    "fuse.js": "^7.1.0",
    "next": "15.4.4",
    "next-contentlayer": "^0.3.4",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
